{
	"compilerOptions": {
		"lib": ["ES2020"],
		"module": "ESNext",
		"target": "ES2020",
		"strict": true,
		"moduleResolution": "Node",
		"isolatedModules": true,
		"noImplicitAny": false,
		"esModuleInterop": true,
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"declaration": true,
		"jsx": "react"
	}
}
